@model IEnumerable<MarkPro.Models.AllUserRequests>

@{
    ViewData["Title"] = "All User Requests";
}

<div class="row">
   @foreach (var item in Model)
    {     
        <div class="card m-1" style="width:350px ">
            <div class="card-body" >
                <h5 class="card-title"><b>Requested By: </b>@Html.DisplayFor(modelItem => item.UserName)</h5>
                <h5 class="card-title"><b>Media Name: </b>@Html.DisplayFor(modelItem => item.MediaName)</h5>
                <h5 class="card-title"><b>Media Type: </b>@Html.DisplayFor(modelItem => item.MediaType)</h5>
             </div>
        </div>
    }
</div>

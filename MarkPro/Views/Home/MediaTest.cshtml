@model IEnumerable<MarkPro.Models.TestModel>


@{
    ViewData["Title"] = "Tests";
}


<div class="row">
   @foreach (var item in Model)
    {
        <div class="card m-1" style="width:350px; box-shadow: 5px 0 5px 0; background-color: #d6e2e2; backdrop-filter: blur(100px); ">
            <div class="card-body" >
                <h5 class="card-title"><b>Name/ID: </b>@Html.DisplayFor(modelItem => item.Name)</h5>
                <h5 class="card-title"><b>Status: </b>@Html.DisplayFor(modelItem => item.CompletionStatus)</h5>
             </div>
        </div>
    }
</div>
